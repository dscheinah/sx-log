{
    "name": "dscheinah/sx-log",
    "description": "Simple error log implementation.",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.3",
        "dscheinah/sx-container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Sx\\Log\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sx\\LogTest\\": "test/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
    }
}
